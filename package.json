    "Micro Service",
    "Go Kit",
    "gRPC",
    "REST",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^2.7.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "nsp": "^3.2.1",
    "prettier": "^1.14.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "chalk": "^2.4.1",
    "global": "^4.3.2",
    "jshint": "^2.9.6",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
